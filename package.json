  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@geckos.io/phaser-on-nodejs": "^1.2.8",
    "@geckos.io/snapshot-interpolation": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "easystarjs": "^0.4.4",
    "express": "^4.18.2",
    "mongodb": "^5.1.0",
    "phaser": "^3.55.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^5.10.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "theme-ui": "^0.15.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "esbuild": "^0.17.11",
    "esbuild-loader": "^3.0.1",
    "nodemon": "^2.0.21"
  },
  "scripts": {
    "kill": "kill -9 $(lsof -i :3000) &>/dev/null | exit 0",
    "server": "ts-node -r dotenv/config --files server/index.ts",
    "watch:server": "nodemon --watch \"server/**\" --ext \"ts,js,json\" --inspect --exec \"yarn run kill && yarn run server\"",
    "watch": "nodemon --watch src node esbuild.js",
    "build": "node esbuild.js "
  }
