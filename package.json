{
  "name": "aether",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "tsc",
    "client:dev": "vite client",
    "client:build": "cd client && vite build",
    "client:preview": "cd client && vite preview",
    "server:kill": "kill -9 $(lsof -i :3000) &>/dev/null | exit 0",
    "server:run": "ts-node -r dotenv/config --files server/index.ts",
    "server:watch": "nodemon --watch \"server/**\" --watch \"shared/**\" --ext \"ts,js,json\" --inspect --exec \"yarn run server:kill && yarn run server:run\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@geckos.io/phaser-on-nodejs": "^1.2.8",
    "@geckos.io/snapshot-interpolation": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "easystarjs": "^0.4.4",
    "eslint": "^8.43.0",
    "express": "^4.18.2",
    "framer-motion": "^10.12.18",
    "mongodb": "^5.6.0",
    "nanoid": "^4.0.2",
    "phaser": "^3.55.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^5.16.1",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "theme-ui": "^0.15.5",
    "use-double-tap": "^1.3.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "nodemon": "^2.0.21",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.3.9"
  }
}
