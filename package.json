{
  "name": "aether",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "kill": "kill -9 $(lsof -i :3000) &>/dev/null | exit 0",
    "server": "ts-node -r dotenv/config --files server/index.ts",
    "watch:server": "nodemon --watch \"server/**\" --watch \"shared/**\" --ext \"ts,js,json\" --inspect --exec \"yarn run kill && yarn run server\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@emotion/react": "^11.10.6",
    "@geckos.io/phaser-on-nodejs": "^1.2.8",
    "@geckos.io/snapshot-interpolation": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "easystarjs": "^0.4.4",
    "express": "^4.18.2",
    "framer-motion": "^10.10.0",
    "mongodb": "^5.1.0",
    "phaser": "^3.55.2",
    "react-tooltip": "^5.10.0",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0",
    "theme-ui": "^0.15.5",
    "use-double-tap": "^1.3.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "ts-node": "^10.9.1",
    "nodemon": "^2.0.21"
  }
}
