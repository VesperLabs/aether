{
  "name": "aether",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "tsc",
    "client:dev": "vite client",
    "client:build": "cd client && vite build",
    "client:preview": "cd client && vite preview",
    "web:dev": "vite web",
    "web:build": "cd web && vite build",
    "web:preview": "cd web && vite preview",
    "server:kill:unix": "kill -9 $(lsof -i :3000) &>/dev/null | exit 0",
    "server:kill:win": "",
    "server:kill": "node -e \"require('child_process').execSync(process.platform.startsWith('win') ? 'npm run server:kill:win' : 'npm run server:kill:unix')\"",
    "server:dev": "ts-node -r dotenv/config --files server/main.ts",
    "server:watch": "nodemon --watch \"server/**\" --watch \"shared/**\" --ext \"ts,js,json\" --inspect --exec \"npm run server:kill && npm run server:dev\"",
    "clean": "rm -rf node_modules",
    "server:build": "node -r dotenv/config build.server.js",
    "server:run": "node -r dotenv/config --optimize-for-size --max-old-space-size=512 dist/server.js",
    "test": "vitest"
  },
  "workspaces": [
    "ui",
    "web",
    "shared",
    "client"
  ],
  "dependencies": {
    "@capacitor/core": "^6.1.2",
    "@capacitor/ios": "^6.1.2",
    "@geckos.io/snapshot-interpolation": "^1.1.0",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "easystarjs": "^0.4.4",
    "eslint": "^9.14.0",
    "express": "^4.21.1",
    "framer-motion": "^11.11.17",
    "http-proxy-middleware": "^3.0.3",
    "jsdom": "^25.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.0.0",
    "nanoid": "^5.0.8",
    "peer": "^1.0.2",
    "peerjs": "^1.5.4",
    "phaser": "3.55.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "react-tooltip": "^5.28.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "theme-ui": "^0.17.1",
    "use-double-tap": "^1.3.6"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.1.2",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "esbuild": "^0.19.2",
    "husky": "^8.0.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "typescript": "^5.6.3",
    "vite": "^4.3.9",
    "vitest": "^2.1.5"
  }
}
